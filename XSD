              <xs:element name="premios" type="xs:integer"/>
              <xs:element name="sinopsis" type="xs:string"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
